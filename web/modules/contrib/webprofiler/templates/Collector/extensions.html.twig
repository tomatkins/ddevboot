{% block toolbar %}

  {% set icon %}
    {{ include('@webprofiler/Icon/010--estensions.svg') }}
    <span class="sf-toolbar-value">{{ collector.getExtensionsCount }}</span>
  {% endset %}

  {% set text %}
    <div class="sf-toolbar-info-piece">
      <b>{{ 'Modules'|t }}</b>
      <span class="sf-toolbar-status">{{ collector.getModulesCount }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
      <b>{{ 'Themes'|t }}</b>
      <span class="sf-toolbar-status">{{ collector.getThemesCount }}</span>
    </div>
  {% endset %}

  {{ include('@webprofiler/Profiler/toolbar_item.html.twig', { link: profiler_url }) }}
{% endblock %}

{% block panel %}
  {{ collector.panel() }}
{% endblock %}
