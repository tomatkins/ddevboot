{% block toolbar %}

  {% set icon %}
    {{ include('@webprofiler/Icon/004--assets.svg') }}
    <span class="sf-toolbar-value">{{ collector.getcsscount + collector.getjscount }}</span>
  {% endset %}

  {% set text %}
    <div class="sf-toolbar-info-piece">
      <b>CSS</b>
      <span class="sf-toolbar-status">{{ collector.getcsscount }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
      <b>JS</b>
      <span class="sf-toolbar-status">{{ collector.getjscount }}</span>
    </div>
  {% endset %}

  {{ include('@webprofiler/Profiler/toolbar_item.html.twig', { link: profiler_url }) }}
{% endblock %}

{% block panel %}
  {{ collector.panel() }}
{% endblock %}
